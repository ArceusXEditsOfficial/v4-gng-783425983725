-- Scripts:

local function SDJGC_fake_script() -- GhxstUniversalHub.ToggleButton.LocalScript 
	local script = Instance.new('LocalScript', GhxstUniversalHub.ToggleButton)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	local toggleButton = script.Parent 
	local mainScreenGui = player:WaitForChild("PlayerGui"):WaitForChild("Ghxst Universal Hub") 
	
	local function toggleScreenGui()
		mainScreenGui.Enabled = not mainScreenGui.Enabled
	end
	
	toggleButton.MouseButton1Click:Connect(toggleScreenGui)
	
end
coroutine.wrap(SDJGC_fake_script)()
local function GOCDZ_fake_script() -- GhxstUniversalHub.MainFrame.Drag 
	local script = Instance.new('LocalScript', GhxstUniversalHub.MainFrame)

	local TweenService = game:GetService("TweenService")
	local UserInputService = game:GetService("UserInputService")
	
	local frame = script.Parent
	
	local dragging = false
	local dragInput, mousePos, framePos
	local lastMousePos, lastGoalPos
	local dragSpeed = 0.10  -- smoothness
	
	local function lerp(a, b, m)
		return a + (b - a) * m
	end
	
	local function update(input)
		local delta = input.Position - mousePos
		local newPos = UDim2.new(
			framePos.X.Scale,
			framePos.X.Offset + delta.X,
			framePos.Y.Scale,
			framePos.Y.Offset + delta.Y
		)
		lastGoalPos = newPos
	end
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			mousePos = input.Position
			framePos = frame.Position
			lastMousePos = mousePos
			lastGoalPos = frame.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if dragging and input == dragInput then
			update(input)
		end
	end)
	
	game:GetService("RunService").RenderStepped:Connect(function()
		if dragging then
			frame.Position = UDim2.new(
				lerp(frame.Position.X.Scale, lastGoalPos.X.Scale, dragSpeed),
				lerp(frame.Position.X.Offset, lastGoalPos.X.Offset, dragSpeed),
				lerp(frame.Position.Y.Scale, lastGoalPos.Y.Scale, dragSpeed),
				lerp(frame.Position.Y.Offset, lastGoalPos.Y.Offset, dragSpeed)
			)
		end
	end)
	
end
coroutine.wrap(GOCDZ_fake_script)()
local function NTTP_fake_script() -- GhxstUniversalHub.MainFrame.Tabs System Handler 
	local script = Instance.new('LocalScript', GhxstUniversalHub.MainFrame)

	local screenGui = script.Parent.Parent
	local mainFrame = script.Parent
	local tabButtonsFrame = mainFrame:WaitForChild("TabButtonsFrame")
	local defaultButtonTextColor = Color3.fromRGB(192, 192, 192)
	local selectedButtonTextColor = Color3.fromRGB(255, 255, 255)
	
	local function showTab(tabNumber)
		for i = 1, 50 do
			local tabContent = mainFrame:FindFirstChild("Tab" .. i .. "Content")
			local tabButton = tabButtonsFrame:FindFirstChild("Tab" .. i .. "Button")
			if tabContent then
				tabContent.Visible = false
			end
			if tabButton then
				tabButton.TextColor3 = defaultButtonTextColor
			end
		end
	
		local selectedTabContent = mainFrame:FindFirstChild("Tab" .. tabNumber .. "Content")
		local selectedTabButton = tabButtonsFrame:FindFirstChild("Tab" .. tabNumber .. "Button")
		if selectedTabContent then
			selectedTabContent.Visible = true
		end
		if selectedTabButton then
			selectedTabButton.TextColor3 = selectedButtonTextColor
		end
	end
	
	for i = 1, 50 do
		local tabButton = tabButtonsFrame:FindFirstChild("Tab" .. i .. "Button")
		if tabButton then
			tabButton.MouseButton1Click:Connect(function()
				showTab(i)
			end)
		end
	end
	
	showTab(1)
	
end
coroutine.wrap(NTTP_fake_script)()
local function PJXY_fake_script() -- GhxstUniversalHub.GhxstUniversalHub.Keybind System 
	local script = Instance.new('LocalScript', GhxstUniversalHub.GhxstUniversalHub)

	
	local screenGui = script.Parent
	local function toggleGuiVisibility()
		screenGui.Enabled = not screenGui.Enabled
	end
	local function onKeyPress(input)
		if input.UserInputType == Enum.UserInputType.Keyboard then
			if input.KeyCode == Enum.KeyCode.LeftControl or input.KeyCode == Enum.KeyCode.RightControl then
				toggleGuiVisibility()
			end
		end
	end
	game:GetService("UserInputService").InputBegan:Connect(onKeyPress)
	
	
	-- Yo, Nailed it
	
end
coroutine.wrap(PJXY_fake_script)()
