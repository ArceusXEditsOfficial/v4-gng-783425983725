GhxstUniversalHub.CollisionsOff.Name = "Collisions Off"
GhxstUniversalHub.CollisionsOff.Parent = GhxstUniversalHub.Buttons_2
GhxstUniversalHub.CollisionsOff.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
GhxstUniversalHub.CollisionsOff.BorderColor3 = Color3.fromRGB(0, 0, 0)
GhxstUniversalHub.CollisionsOff.BorderSizePixel = 0
GhxstUniversalHub.CollisionsOff.Position = UDim2.new(0.512132764, 0, 0.765508294, 0)
GhxstUniversalHub.CollisionsOff.Size = UDim2.new(0, 200, 0, 50)
GhxstUniversalHub.CollisionsOff.Font = Enum.Font.SourceSans
GhxstUniversalHub.CollisionsOff.Text = "Collisions Off"
GhxstUniversalHub.CollisionsOff.TextColor3 = Color3.fromRGB(255, 255, 255)
GhxstUniversalHub.CollisionsOff.TextSize = 14.000
GhxstUniversalHub.CollisionsOff.MouseButton1Down:Connect(function()
    --[[
Thanks for using this script
Want to know how to use this and script explanation? 
Check the video here: https://youtu.be/bGuzzLSzqeQ
]]
	local Players = game:GetService("Players")
	local PS = game:GetService("PhysicsService")

	local CollisionGroupName = "Players"

	PS:CreateCollisionGroup(CollisionGroupName)
	PS:CollisionGroupSetCollidable(CollisionGroupName, CollisionGroupName, false)

	Players.PlayerAdded:Connect(function(Player)
		Player.CharacterAdded:Connect(function(char)

			local function ChangeGroup(Part)
				if Part:IsA("BasePart") then
					PS:SetPartCollisionGroup(Part, CollisionGroupName)
				end
			end

			char.ChildAdded:Connect(ChangeGroup)
			for _, Object in pairs(char:GetChildren()) do
				ChangeGroup(Object)
			end
		end)
	end)
end)

